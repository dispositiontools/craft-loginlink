{
    "name": "dispositiontools/craft-loginlink",
    "description": "Log in with a link",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "loginlink"
    ],
    "support": {
        "docs": "https://github.com/dispositiontools/loginlink/blob/master/README.md",
        "issues": "https://github.com/dispositiontools/loginlink/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Disposition Tools",
            "homepage": "https://www.disposition.tools"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "require-dev": {
      "craftcms/cms": "^3.2.0",
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "dispositiontools\\loginlink\\": "src/"
        }
    },
    "extra": {
        "name": "Log in link",
        "handle": "loginlink",
        "developer": "Disposition Tools",
        "developerUrl": "https://www.disposition.tools",
        "documentationUrl": "https://github.com/dispositiontools/loginlink/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/dispositiontools/loginlink/master/CHANGELOG.md",
        "components": {
            "login": "dispositiontools\\loginlink\\services\\Login"
        },
        "class": "dispositiontools\\loginlink\\Loginlink"
    }
}
